@model IEnumerable<FormRobot.Domain.Entities.AirDropLink>

@{
    ViewBag.Title = "AirDropLinkPage";
}

<h2>AirDropLinkPage</h2>
<table class='table table-hover table-bordered table-striped'>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AirDropLinkUrl)
        </th>
        <th>
             
        </th>
        <th>
            
        </th>
     
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AirDropLinkUrl)
            </td>
            <td>
                @Html.ActionLink("APPLY","Index",new { @id = item.AirDropLinkId },new { @class="btn btn-default" })
            </td>
            <td>
                @item.CreatedDateTime.ToShortDateString()
            </td>
        </tr>
    }

</table>
