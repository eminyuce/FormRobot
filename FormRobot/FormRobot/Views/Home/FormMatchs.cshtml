@model IEnumerable<FormRobot.Domain.Entities.FormMatch>

@{
    ViewBag.Title = "FormMatchs";
}

<h2>FormMatchs</h2>

<p>
    @Html.ActionLink("Create New", "FormMatchItem")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FormItemText)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FormItemKey)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FormItemText)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FormItemKey)
        </td>
        <td>
            @Html.ActionLink("Edit", "FormMatchItem", new { id=item.FormMatchId }) 
        </td>
    </tr>
}

</table>
